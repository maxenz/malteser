<div class="navbar navbar-inverse navbar-fixed-top" role="navigation" id="navbar-top-paramedic">
    <div class="container">
        <div class="navbar-header">
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <a class="navbar-brand title-paramedic" href="#">
                <img src="../../Content/img/paramedic.png" id="logo-paramedic" /></a>
            <p class="navbar-text title-paramedic" style="color: #357ebd">@ViewBag.Title</p>
        </div>

        <div class="navbar-collapse collapse">
            @if (Request.IsAuthenticated)
             {
            <div class="navbar-text pull-right">
                <div class="row">
                    <div class="col-md-1">
                        <i class="fa fa-user azul"></i>@*
                    <span id="user-paramedic">@Session["UserName"]</span>*@
                    </div>
                    <div class="col-md-4">
                        <span id="user-paramedic">mariano</span>
                    </div>
                    <div class="col-md-1">
                        <span id="icons-top-separator">|</span>
                    </div>
                    <div class="col-md-2">

                        @using (Html.BeginForm("LogOff", "Account", FormMethod.Post, new { id = "logoutForm" }))
                        {
                            @Html.AntiForgeryToken()
                            <a href="javascript:document.getElementById('logoutForm').submit()" class="btn btn-default btn-xs" id="navbar-top-logout"><span class="glyphicon glyphicon-off blanco"></span></a>
                        }

                    </div>
                </div>

            </div>
                
             }  
        </div>
    </div>
</div>
